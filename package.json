  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "get-graphql-schema": "^2.1.2",
    "prettier": "^3.3.3",
    "typescript": "5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-typed-graphql": "^3.0.0",
    "vite-plugin-windicss": "^1.9.3",
    "vue-tsc": "^2.1.10"
